package org.mobicrant.iserver.event;

/**
 * Listener interface to handle resource-specific events.
 * @version 1.0 Feb 20, 2006
 * @author Beat Signer, signer@inf.ethz.ch
 */
public interface ResourceListener extends EntityListener {

   void resourceCreated(ResourceEvent e);


   void resourceInitialised(ResourceEvent e);


   void resourceUpdated(ResourceEvent e);


   void resourceDeleted(ResourceEvent e);

}
