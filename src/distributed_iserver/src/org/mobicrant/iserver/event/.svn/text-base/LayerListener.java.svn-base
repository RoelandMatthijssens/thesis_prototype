package org.mobicrant.iserver.event;

/**
 * Listener interface to handle layer-specific events.
 * @version 1.0 Feb 20, 2006
 * @author Beat Signer, signer@inf.ethz.ch
 */
public interface LayerListener {

   void layerCreated(LayerEvent e);


   void layerInitialised(LayerEvent e);


   void layerUpdated(LayerEvent e);


   void layerDeleted(LayerEvent e);
   
   
   void layerMoved(LayerEvent e); 

}
